import requests
from bs4 import BeautifulSoup
import json
from time import sleep
data = []
def parsing():
    url = 'https://mars.nasa.gov/msl/weather/'
    r = requests.get(url)
    soup = BeautifulSoup(r.text, 'lxml')
    soup = soup.find_all('div', class_='textLarger vCentered')
    print(soup)
    for i in soup:
        table = i.find('div', class_='temperatures main vCenteredInner fontBold textLarge').find('div', class_='highs').find('span', class_='text').text
        print(table)
        #for x in table.find_all('tr'):
            #cols = x.find('td', class_='temperature max')
            #print(cols)
parsing()
