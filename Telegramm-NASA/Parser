import requests
from bs4 import BeautifulSoup
from time import sleep
import os
import json
data = []
def parsing():
    url = 'https://mars.nasa.gov/msl/weather/'
    r = requests.get(url)
    soup = BeautifulSoup(r.text, 'lxml')
    soup = soup.find_all('table', class_='mb_table')
    #print(soup)
    for i in soup:
        table = i.find('thead')
        #print(table)
        for x in table:
            cols = x.find('tr')
            print(cols)
parsing()
